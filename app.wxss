/**app.wxss**/
.container {
  background-color:#f2f2f2; 
  position: relative;
  width: 100%;
  height: 100vh;
}
#baiduMap{
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}
.title{
  background-color: #f2f2f2;
  padding: 30rem 0 30rem 50rem;
  font-size: 28rem;
  color: #000
}
.tapbar{
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
  padding: 40rem;
}
.btn-charge{
  width: 90%;
  background-color: #b9dd08;
  margin: 40rem auto 30rem;
  text-align: center;
}